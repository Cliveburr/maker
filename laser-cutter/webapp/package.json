    "@angular/http": "^6.0.6",
    "@angular/platform-browser": "^6.0.6",
    "@angular/platform-browser-dynamic": "^6.0.6",
    "@angular/router": "^6.0.6",
    "core-js": "^2.5.7",
    "rxjs": "^6.2.1",
    "zone.js": "^0.8.26",
    "bootstrap": "^4.1.1",
    "ngx-bootstrap": "^3.0.1",
    "electron": "^2.0.3"
  },
  "devDependencies": {
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.6",
    "@angular-devkit/build-angular": "^0.6.8",
    "typescript": "~2.7.2",
    "@types/node": "^10.3.6"
